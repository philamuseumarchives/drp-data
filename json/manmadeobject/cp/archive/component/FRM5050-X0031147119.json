ve/component/FRM5050-X0031142589", "identified_by": [{"id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589/unittitle", "type": "Name", "value": "Correspondance avec des particuliers"}, {"classified_as": [{"id": "aat:300404670", "label": "preferred terms", "type": "Type"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589/id", "type": "Identifier", "value": "FRM5050-X0031142589"}, {"classified_as": [{"id": "aat:300192339", "label": "sequences", "type": "Type"}], "id": "_:b86", "type": "Identifier", "value": "0"}], "part": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147071", "type": "PhysicalObject"}], "produced_by": {"carried_out_by": ["http://data.duchamparchives.org/cp/archive/actor/local/duchamp-marcel-1887-1968-", "http://data.duchamparchives.org/cp/archive/actor/local/brancusi-constantin-1876-1957-", "http://data.duchamparchives.org/cp/archive/actor/local/morand-paul-1887-1976-"], "consists_of": [{"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/local/duchamp-marcel-1887-1968-", "label": "Duchamp, Marcel (1887-1968)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147119/production/auteur", "technique": [{"id": "aat:author", "label": "author", "type": "Type"}], "type": "Production"}, {"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/local/morand-paul-1887-1976-", "label": "Morand, Paul (1887-1976)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147119/production/sujet", "technique": [{"id": "aat:300248231", "label": ["depicted", "depictions (visual works)"], "type": "Type"}], "type": "Production"}, {"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/local/brancusi-constantin-1876-1957-", "label": "Brancusi, Constantin (1876-1957)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147119/production/destinataire", "technique": [{"id": "aat:recipient", "label": "recipient", "type": "Type"}], "type": "Production"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147119/production", "timespan": {"begin_of_the_begin": "1926-01-01T00:00:00+00:00", "end_of_the_end": "1926-01-01T00:00:00+00:00", "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147119/production/timespan", "label": "1926", "type": "TimeSpan"}, "type": "Production"}, "referred_to_by": [{"classified_as": ["aat:scopenote"], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147119/scope", "type": "LinguisticObject", "value": "&lt;p&gt;Monaco 13 sept. [1926], &lt;/p&gt;\n         &lt;p&gt;H\u00f4tel de Nice jusqu'au 25 sept. 1926&lt;/p&gt;\n         &lt;p&gt;Cher Maurice&lt;/p&gt;\n         &lt;p&gt;Bien re\u00e7u ton mot de Plymouth qui annon\u00e7ait un bon voyage depuis longtemps termin\u00e9 pour toi. Ici tout marche bien. Je suis \u00e0 Monaco jusqu'\u00e0 la fin du mois.&lt;/p&gt;\n         &lt;p&gt;J'ai re\u00e7u la pr\u00e9face de Morand qui est tr\u00e8s bien. Je la fais traduire et te l'enverrai.&lt;/p&gt;\n         &lt;p&gt;J'ai donn\u00e9 avant de partir tous les dessins \u00e0 encadrer \u00e0 Foinet. Tout sera pr\u00eat ainsi que l'emballage de tes sculptures quand j'arriverai \u00e0 Paris. Tout s'annonce tr\u00e8s bien. J'ai re\u00e7u une lettre de mon ami Allen Norton chez qui je dois habiter \u00e0 New-York. Tu le rencontreras chez Walter Arensberg et il nous aidera pour la publicit\u00e9.&lt;/p&gt;\n         &lt;p&gt;Si tu vois Mme Rumsey demande lui de d\u00e9poser 1500 dollars \u00e0 ma banque 530 Fifth Avenue pour que je puisse terminer le paiement \u00e0 Brummer. &lt;/p&gt;\n         &lt;p&gt;\u00c0 bient\u00f4t&lt;/p&gt;\n         &lt;p&gt;Tr\u00e8s affectueux &lt;/p&gt;\n         &lt;p&gt;Maurice&lt;/p&gt;\n         &lt;p&gt;29 rue Camp. Premi\u00e8re&lt;/p&gt;"}, {"classified_as": [{"id": "aat:300266036", "label": "dimensions", "type": "Type"}], "type": "LinguisticObject", "value": "lettre manuscrite\n1 f."}], "refers_to": [], "related": [], "representation": [], "subject_of": [{"id": "http://archivesetdocumentation.centrepompidou.fr/img-viewer/BK/BRAN/BRAN_5_1_17dir.json", "type": "InformationObject"}], "type": "ManMadeObject", "used_for": []}