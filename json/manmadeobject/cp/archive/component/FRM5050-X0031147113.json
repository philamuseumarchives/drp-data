e"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589", "identified_by": [{"id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589/unittitle", "type": "Name", "value": "Correspondance avec des particuliers"}, {"classified_as": [{"id": "aat:300404670", "label": "preferred terms", "type": "Type"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589/id", "type": "Identifier", "value": "FRM5050-X0031142589"}, {"classified_as": [{"id": "aat:300192339", "label": "sequences", "type": "Type"}], "id": "_:b86", "type": "Identifier", "value": "0"}], "part": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147071", "type": "PhysicalObject"}], "produced_by": {"carried_out_by": ["http://data.duchamparchives.org/cp/archive/actor/local/brancusi-constantin-1876-1957-", "http://data.duchamparchives.org/cp/archive/actor/local/duchamp-marcel-1887-1968-", "http://data.duchamparchives.org/cp/archive/actor/local/roch-henri-pierre-1879-1959-"], "consists_of": [{"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/local/roch-henri-pierre-1879-1959-", "label": "Roch\u00e9, Henri-Pierre (1879-1959)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147113/production/sujet", "technique": [{"id": "aat:300248231", "label": ["depicted", "depictions (visual works)"], "type": "Type"}], "type": "Production"}, {"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/local/brancusi-constantin-1876-1957-", "label": "Brancusi, Constantin (1876-1957)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147113/production/destinataire", "technique": [{"id": "aat:recipient", "label": "recipient", "type": "Type"}], "type": "Production"}, {"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/local/duchamp-marcel-1887-1968-", "label": "Duchamp, Marcel (1887-1968)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147113/production/auteur", "technique": [{"id": "aat:author", "label": "author", "type": "Type"}], "type": "Production"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147113/production", "timespan": {"begin_of_the_begin": "1926-01-01T00:00:00+00:00", "end_of_the_end": "1926-01-01T00:00:00+00:00", "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147113/production/timespan", "label": "1926", "type": "TimeSpan"}, "type": "Production"}, "referred_to_by": [{"classified_as": [{"id": "aat:300266036", "label": "dimensions", "type": "Type"}], "type": "LinguisticObject", "value": "Lettre manuscrite \u00e0 l'encre rouge\n1 f."}, {"classified_as": ["aat:scopenote"], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147113/scope", "type": "LinguisticObject", "value": "&lt;p&gt;13 f\u00e9vrier&lt;/p&gt;\n         &lt;p&gt;Je re\u00e7ois ta lettre ce matin, j'y r\u00e9ponds \u00e0 midi.&lt;/p&gt;\n         &lt;p&gt;Je vais t\u00e9l\u00e9phoner \u00e0 Roch\u00e9 de te c\u00e2bler l'adresse de Mme Foster.&lt;/p&gt;\n         &lt;p&gt;Enchant\u00e9 d'avoir de tes nouvelles. &lt;/p&gt;\n         &lt;p&gt;Je savais que tu avais eu un voyage \u00e9pouvantable - tu me raconteras cela en d\u00e9tails. &lt;/p&gt;\n         &lt;p&gt;Ne t'inqui\u00e8te de rien \u00e0 Paris. Je vais voir L\u00e9ger pour les Ind\u00e9pendants. &lt;/p&gt;\n         &lt;p&gt;Ton courrier n'a rien d'important&lt;/p&gt;\n         &lt;p&gt;J'ai pay\u00e9 tes contributions.&lt;/p&gt;\n         &lt;p&gt;Reste aussi longtemps que tu voudras et vends beaucoup.&lt;/p&gt;\n         &lt;p&gt;Je regrette de n'\u00eatre pas avec toi.&lt;/p&gt;\n         &lt;p&gt;Bien mes amiti\u00e9s \u00e0 tout New York et \u00e0 bient\u00f4t.&lt;/p&gt;\n         &lt;p&gt;Marcel&lt;/p&gt;\n         &lt;p&gt;Wildenstein est une tr\u00e8s bonne galerie.&lt;/p&gt;"}], "refers_to": [], "related": [], "representation": [], "subject_of": [{"id": "http://archivesetdocumentation.centrepompidou.fr/img-viewer/BK/BRAN/BRAN_5_1_11dir.json", "type": "InformationObject"}], "type": "ManMadeObject", "used_for": []}