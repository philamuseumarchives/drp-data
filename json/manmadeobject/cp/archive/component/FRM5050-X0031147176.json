, "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147142", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147167", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147132", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147164", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147150", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147114", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147135", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147146", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147126", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147158", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147173", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147123", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147105", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147185", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147179", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147137", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147170", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147166", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147194", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147155", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147195", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147189", "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147141"], "type": "PhysicalObject"}, {"classified_as": [{"id": "aat:series", "label": "series", "type": "Type"}, {"id": "aat:300375748", "label": "archives (groupings)", "type": "Type"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589", "identified_by": [{"id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589/unittitle", "type": "Name", "value": "Correspondance avec des particuliers"}, {"classified_as": [{"id": "aat:300404670", "label": "preferred terms", "type": "Type"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031142589/id", "type": "Identifier", "value": "FRM5050-X0031142589"}, {"classified_as": [{"id": "aat:300192339", "label": "sequences", "type": "Type"}], "id": "_:b86", "type": "Identifier", "value": "0"}], "part": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147071", "type": "PhysicalObject"}], "produced_by": {"carried_out_by": ["http://data.duchamparchives.org/cp/archive/actor/isni/0000000107834751", "http://data.duchamparchives.org/cp/archive/actor/isni/0000000121280214"], "consists_of": [{"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/isni/0000000121280214", "label": "Duchamp, Marcel (1887-1968)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147176/production/auteur", "technique": [{"id": "aat:author", "label": "author", "type": "Type"}], "type": "Production"}, {"carried_out_by": [{"id": "http://data.duchamparchives.org/cp/archive/actor/isni/0000000107834751", "label": "Brancusi, Constantin (1876-1957)", "type": "Actor"}], "classified_as": [], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147176/production/destinataire", "technique": [{"id": "aat:recipient", "label": "recipient", "type": "Type"}], "type": "Production"}], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147176/production", "timespan": {"begin_of_the_begin": "1934-01-01T00:00:00+00:00", "end_of_the_end": "1934-01-01T00:00:00+00:00", "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147176/production/timespan", "label": "1934", "type": "TimeSpan"}, "type": "Production"}, "referred_to_by": [{"classified_as": ["aat:scopenote"], "id": "http://data.duchamparchives.org/cp/archive/component/FRM5050-X0031147176/scope", "type": "LinguisticObject", "value": "&lt;p&gt;N. Y. 9 Janvier 34&lt;/p&gt;\n         &lt;p&gt;Cher Morice&lt;/p&gt;\n         &lt;p&gt;Comme mon c\u00e2ble te l'a annonc\u00e9 nous avons vendu la N\u00e9gresse blonde \u00e0 Mr Goodwin qui en fait cadeau au Mus\u00e9e de Hartford. Il n'a pas pris le Nouveau n\u00e9 en acier, je ne sais pas pourquoi, au fond c'est mieux pour un mus\u00e9e. (1600 dollars - 10% = 1440 dollars \u00e0 16.35 environ = 23 500 frs environ r\u00e9serv\u00e9s \u00e0 la banque de Brummer).&lt;/p&gt;\n         &lt;p&gt;Le m\u00eame jour nous avons vendu le pl\u00e2tre de la Muse endormie que Madame Rumsey avait rapport\u00e9 en \u00e9change du marbre qu'elle paie 1000 dollars (nous ne changeons pas ces 1000 dollars parce qu'on ne sait pas quand elle paiera). Nous avons vendu le pl\u00e2tre \u00e0 Mme Guggenheimer (rien de commun avec Peggy) qui a pay\u00e9 $ 250 (chang\u00e9s aussi \u00e0 16,35 le dollar). Te rappelles-tu d'o\u00f9 vient ce pl\u00e2tre (collection Quinn ? ou autre ?) ?&lt;/p&gt;\n         &lt;p&gt;Maintenant j'ai vu Speiser qui va s'occuper de vendre Leda au Mus\u00e9e de Philadelphia ; j'ai pris sur moi d'accorder une r\u00e9duction de 500 dollars, c-\u00e0-d que le prix pour le mus\u00e9e de Philadelphie serait 3000 dollars avec paiements \u00e9chelonn\u00e9s si cela facilite la vente. Sur le conseil de Speiser, j'ai envoy\u00e9 une photo de Leda \u00e0 Ingersoll qui lui aussi se joint \u00e0 Speiser pour faire l'affaire du Mus\u00e9e de Philadelphie. Ils esp\u00e8rent trouver l'argent parmi les membres du comit\u00e9.&lt;/p&gt;\n         &lt;p&gt;J'ai vu aujourd'hui Mme Meyer avec sa 3\u00e8me fille ; elle doit revenir avec son mari demain ou apr\u00e8s-demain pour, j'esp\u00e8re, le d\u00e9cider \u00e0 discuter le prix ou accepter 4000 $. Comme tu nous l'as dit, nous les laissons faire comme ils veulent.&lt;/p&gt;\n         &lt;p&gt;En somme l'exposition a bien march\u00e9 et si tu te rendais compte de l'\u00e9tat de choses ici, tu devrais \u00eatre satisfait p\u00e9cuniairement.&lt;/p&gt;\n         &lt;p&gt;Au point de vue succ\u00e8s g\u00e9n\u00e9ral, tout le monde est d'accord pour dire que c'est la meilleure exposition de l'ann\u00e9e et de loin.&lt;/p&gt;\n         &lt;p&gt;Chicago a enfin r\u00e9pondu qu'ils ne peuvent pas supporter les frais et je prends le \"Champlain\" le 20 janvier pour \u00eatre \u00e0 Paris le 27.&lt;/p&gt;\n         &lt;p&gt;Affectueusement&lt;/p&gt;\n         &lt;p&gt;\u00c0 bient\u00f4t &lt;/p&gt;\n         &lt;p&gt;Morice&lt;/p&gt;"}, {"classified_as": [{"id": "aat:300266036", "label": "dimensions", "type": "Type"}], "type": "LinguisticObject", "value": "Lettre manuscrite\n2 f."}], "refers_to": [], "related": [], "representation": [], "subject_of": [{"id": "http://archivesetdocumentation.centrepompidou.fr/img-viewer/BK/BRAN/BRAN_5_1_74dir.json", "type": "InformationObject"}], "type": "ManMadeObject", "used_for": []}